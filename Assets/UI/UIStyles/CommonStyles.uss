.menuScreen {
    width: 100%;
    height: 100%;
    justify-content: center;
    align-items: center;
    display: flex;
    background-color: rgb(0, 0, 0);
}

.resourceBar {
    width: 92%;
    background-color: rgb(135, 41, 41);
    align-items: center;
    height: 15px;
    flex-direction: row;
    margin-left: 4%;
    margin-right: 4%;
    margin-top: 0;
    margin-bottom: 0;
}

.barContainer {
    width: 100%;
    background-image: url('project://database/Assets/Sprites/UI/bar.png?fileID=2800000&guid=a3134fd38065d07469d6656123260a7e&type=3#bar');
    -unity-background-scale-mode: stretch-to-fill;
    height: 32px;
    background-color: rgb(0, 0, 0);
    justify-content: center;
    align-items: center;
}

.barMissingAmount {
    width: 0;
    height: 15px;
    background-color: rgb(56, 56, 56);
    justify-content: center;
    overflow: visible;
}

.barInteractionResult {
    width: 0;
    height: 15px;
    background-color: rgb(255, 0, 0);
    justify-content: center;
    visibility: visible;
}

.barText {
    position: absolute;
}

.healthLabel {
    font-size: 18px;
    color: rgb(255, 255, 255);
    background-image: url('project://database/Assets/Sprites/Icons/cure.png?fileID=2800000&guid=74c108c1392ffdb4eb4e79bd11f34e57&type=3#cure');
    width: 32px;
    height: 32px;
}

.manaLabel {
    font-size: 18px;
    color: rgb(255, 255, 255);
    background-image: url('project://database/Assets/Sprites/Icons/charm.png?fileID=2800000&guid=15968a194dfe51b49bc2a583a4957492&type=3#charm');
    width: 32px;
    height: 32px;
}

.characteristicGroup {
    width: 50%;
    align-items: flex-start;
    justify-content: center;
    flex-grow: 1;
    flex-shrink: 0;
    flex-basis: initial;
    flex-wrap: nowrap;
    flex-direction: column;
}

.characterCard {
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    width: 100%;
}

.statsGroup {
    flex-direction: row;
    justify-content: center;
    width: 100%;
    margin-top: 5px;
    height: auto;
    flex-wrap: wrap;
}

.statValue {
    font-size: 24px;
    color: rgb(255, 255, 255);
    background-image: none;
    width: 32px;
    height: 32px;
    -unity-text-outline-width: 0;
    text-shadow: 1px 1px 0 rgb(0, 0, 0);
    -unity-text-align: middle-center;
}

.statIcon {
    font-size: 18px;
    color: rgb(255, 255, 255);
    background-image: url('project://database/Assets/Sprites/Icons/gorilla.png?fileID=2800000&guid=c1181cbabffb1da4cbfd479b065b8e49&type=3#gorilla');
    width: 32px;
    height: 32px;
}

.modifierContainer {
    height: 32px;
    width: 100%;
    flex-direction: row;
    flex-wrap: wrap;
}

.modifierIcon {
    width: 32px;
    height: 32px;
    background-image: url('project://database/Assets/Sprites/Icons/armorDown.png?fileID=2800000&guid=0fda0a682cfb53c4aa27458455162b24&type=3#armorDown');
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
}

.textPrimary {
    color: rgb(255, 255, 255);
    font-size: 24px;
    -unity-font: url('project://database/Assets/UI/Pixellari.ttf?fileID=12800000&guid=e2ef6fe3a5f2ad741bd809f9098af174&type=3#Pixellari');
    -unity-font-definition: none;
}

.textSecondary {
    color: rgb(255, 255, 255);
    font-size: 18px;
    -unity-font: url('project://database/Assets/UI/Pixellari.ttf?fileID=12800000&guid=e2ef6fe3a5f2ad741bd809f9098af174&type=3#Pixellari');
    -unity-font-definition: none;
}

.abilityDescription {
    color: rgb(255, 255, 255);
}

.abilityManaCost {
    color: rgb(255, 255, 255);
}

.uiContainer {
    background-image: url('project://database/Assets/Sprites/UI/confirm_bg.png?fileID=21300000&guid=569d6618661c80748874fc29de25b9c1&type=3#confirm_bg');
    -unity-background-scale-mode: stretch-to-fill;
    padding-left: 25px;
    padding-right: 25px;
    padding-top: 25px;
    padding-bottom: 25px;
    min-width: 90px;
    min-height: 90px;
    align-items: flex-start;
    justify-content: center;
    background-color: rgba(0, 0, 0, 0.01);
}

.menuButton {
    font-size: 36px;
    background-color: rgba(255, 255, 255, 0);
    margin-bottom: 5px;
    margin-top: 5px;
    color: rgb(255, 255, 255);
    border-left-color: rgb(255, 133, 125);
    border-right-color: rgb(255, 133, 125);
    border-top-color: rgb(255, 133, 125);
    border-bottom-color: rgb(255, 133, 125);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    transition-duration: 0.5s;
    margin-left: 0;
    margin-right: 0;
    background-image: url('project://database/Assets/Sprites/UI/button.png?fileID=21300000&guid=d4853c58909ab054aa116dca71859aca&type=3#button');
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    width: 400px;
    height: 90px;
    transition-property: scale;
    -unity-text-outline-color: rgb(255, 255, 255);
    -unity-text-outline-width: 0;
    -unity-font-style: normal;
    -unity-font: url('project://database/Assets/UI/Pixellari.ttf?fileID=12800000&guid=e2ef6fe3a5f2ad741bd809f9098af174&type=3#Pixellari');
    -unity-font-definition: none;
}

.menuButton:hover {
    transition-property: scale;
    scale: 1.05 1.05;
    transition-duration: 0.5s;
    transition-timing-function: ease-in-out-sine;
    background-image: url('project://database/Assets/Sprites/UI/button.png?fileID=21300000&guid=d4853c58909ab054aa116dca71859aca&type=3#button');
}

.menuButton:active {
    transition-property: scale;
    scale: 0.95 0.95;
    transition-duration: 0.5s;
    transition-timing-function: ease-in-out-sine;
    background-image: url('project://database/Assets/Sprites/UI/buttonHover.png?fileID=21300000&guid=812248e78729459499b34488a1dd541a&type=3#buttonHover');
}

.menuButton:disabled {
    background-image: url('project://database/Assets/Sprites/UI/buttonDisabled.png?fileID=2800000&guid=ef69fcec9e4ee184881eaa7d860ca33e&type=3#buttonDisabled');
}

.characterCardPortrait {
    -unity-background-scale-mode: scale-and-crop;
    flex-direction: row;
    align-items: flex-end;
    min-width: 120px;
    min-height: 120px;
}

.disabledMenuButton {
    background-image: url('project://database/Assets/Sprites/UI/buttonDisabled.png?fileID=2800000&guid=ef69fcec9e4ee184881eaa7d860ca33e&type=3#buttonDisabled');
}

.characterCardDetailsButton {
    width: 25px;
    height: 25px;
    background-image: url('project://database/Assets/Sprites/UI/info.png?fileID=2800000&guid=ea6c8c25a5d12f4478c249fcb1d701f2&type=3#info');
    background-color: rgb(255, 255, 255);
}

.abilityButton {
    height: 100%;
    width: 120px;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    max-height: 150px;
}

.abilityButtonIcon {
    width: 100%;
    height: 100%;
    background-image: url('project://database/Assets/Sprites/Icons/sword.png?fileID=21300000&guid=64e29a7dee046c448b82b1c27419f427&type=3#sword');
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 2px;
    padding-bottom: 2px;
}

.abilityButtonStat {
}

.itemSlot {
    border-left-color: rgb(89, 91, 147);
    border-right-color: rgb(89, 91, 147);
    border-top-color: rgb(89, 91, 147);
    border-bottom-color: rgb(89, 91, 147);
    border-left-width: 4px;
    border-right-width: 4px;
    border-top-width: 4px;
    border-bottom-width: 4px;
    width: 75px;
    height: 75px;
    background-color: rgb(72, 72, 72);
    align-items: center;
    justify-content: space-between;
    margin-left: 2px;
    margin-right: 2px;
    margin-top: 2px;
    margin-bottom: 2px;
}

.itemSlot:hover {
    border-left-color: rgb(130, 116, 63);
    border-right-color: rgb(130, 116, 63);
    border-top-color: rgb(130, 116, 63);
    border-bottom-color: rgb(130, 116, 63);
    border-left-width: 4px;
    border-right-width: 4px;
    border-top-width: 4px;
    border-bottom-width: 4px;
    transition-duration: 0.5s;
}
